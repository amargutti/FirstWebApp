{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Default": "Data Source=(LocalDb)\\FirstWebApp;Database=FirstWebAppDB;Trusted_Connection=True;"
  },
  "Courses": {
    "PerPage": 10,
    "Order": {
      "By": "Rating",
      "Ascending": false,
      "Allow": ["Title", "Rating", "CurrentPrice"]
    }
  }
}
